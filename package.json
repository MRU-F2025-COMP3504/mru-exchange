{
  "name": "mru-exchange",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prune": "docker system prune -f",
    "docker": "docker compose -f compose.dev.yml up",
    "docker:build": "docker compose -f compose.dev.yml up --build",
    "docker:prune": "docker system prune -f",
    "deploy": "git checkout stable && git pull origin stable && docker stop production && docker system prune -f && docker compose up -d --build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/vite": "^4.1.17",
    "@types/jsonwebtoken": "^9.0.10",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-dom": "^2.0.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-react-x": "^2.0.6",
    "eslint-plugin-tsdoc": "^0.5.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supabase": "^2.51.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.10"
  }
